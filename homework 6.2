#include <iostream>
using namespace std;

int main() {
    int arr[3][3];
    int sum = 0;
    int num = 1;
    
    cout << "Элементы массива:" << endl;
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            arr[i][j] = num;
            cout << num << " ";
            sum += num;
            num++;
        }
        cout << endl;
    }
    
    cout << endl << "Сумма элементов: " << sum << endl;
    
    return 0;
}
